    "description": "D binding for the poppler PDF rendering library",
    "license": "MIT",
    "name": "doppler",
    "preGenerateCommands": [ "$PACKAGE_DIR/pregen" ],
    "configurations": [
        {
            "name": "lib",
            "targetType": "staticLibrary",
            "lflags-linux": [
                "-L$PACKAGE_DIR/3rdparty/poppler/build",
                "-L$PACKAGE_DIR/3rdparty/poppler/glib/build",
                "-lpoppler-glib",
                "-lgobject-2.0",
                "-lglib-2.0"
            ],
            "lflags-osx": [
                "-L$PACKAGE_DIR/3rdparty/poppler/build",
                "-L$PACKAGE_DIR/3rdparty/poppler/glib/build",
                "-lpoppler-glib",
                "-lgobject-2.0",
                "-lglib-2.0"
            ],
            "lflags-windows": [
                "-L$PACKAGE_DIR/3rdparty/poppler/build",
                "-L$PACKAGE_DIR/3rdparty/poppler/glib/build",
                "-lpoppler-glib",
                "-lgobject-2.0",
                "-lglib-2.0"
            ]
        }
    ]
}
